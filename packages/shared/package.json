{
  "name": "@szg/morpher-client-shared",
  "version": "2.1.2",
  "description": "The shared module of the Morpher Client application that contains translations and HTTP services",
  "private": true,
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "jest --maxWorkers=4",
    "test:watch": "jest --watch",
    "build": "echo Do nothing"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "jest-fetch-mock": "3.0.3"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": ["./jest.setup.js"]
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.2",
    "yarn": ">=1.22.18"
  },
  "keywords": [
    "morpher",
    "inflection",
    "morphological analysis",
    "morpher"
  ],
  "bugs": {
    "url": "https://github.com/szgabsz91/morpher-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szgabsz91/morpher-client"
  },
  "author": {
    "name": "Gabor Szabo",
    "email": "szgabsz91@gmail.com",
    "url": "https://github.com/szgabsz91"
  },
  "license": "MIT"
}
