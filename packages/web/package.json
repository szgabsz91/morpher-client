{
  "name": "@szg/morpher-client-web",
  "version": "2.1.1",
  "description": "The web version of the Morpher Client application",
  "private": true,
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "react-scripts test --watchAll=false --coverage --maxWorkers=4",
    "test:watch": "react-scripts test",
    "build": "react-scripts build",
    "start": "react-scripts start"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@szg/morpher-client-shared": "2.1.1",
    "clsx": "1.2.1",
    "formik": "2.2.9",
    "i18next": "21.8.13",
    "i18next-browser-languagedetector": "6.1.4",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.18.0",
    "react-router-dom": "6.3.0",
    "react-window": "1.8.7",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "react-scripts": "5.0.1"
  },
  "proxy": "http://localhost:8080",
  "jest": {
    "clearMocks": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.13.1",
    "yarn": ">=1.22.18"
  },
  "keywords": [
    "morpher",
    "inflection",
    "morphological analysis",
    "morpher"
  ],
  "bugs": {
    "url": "https://github.com/szgabsz91/morpher-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szgabsz91/morpher-client"
  },
  "author": {
    "name": "Gabor Szabo",
    "email": "szgabsz91@gmail.com",
    "url": "https://github.com/szgabsz91"
  },
  "license": "MIT"
}
